{
  "name": "back-office",
  "version": "0.0.0",
  "description": "Ats Back Office",
  "main": "src/js/js.js",
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean",
    "install-app": "npm run install-bower && npm run install-gulp && npm run install-deps && npm run install-compass",
    "install-bower": "sudo npm install bower -g",
    "install-gulp": "sudo npm install gulp -g",
    "install-deps": "npm install && bower install",
    "install-compass": "gem update --system && gem install compass",
    "install-compass-fix": "rvm osx-ssl-certs update all"
  },
  "author": "James Hill",
  "license": "MIT",
  "devDependencies": {
    "async": "^0.8.0",
    "chalk": "^0.4.0",
    "connect-livereload": "^0.4.0",
    "deamdify": "^0.1.1",
    "debowerify": "^0.7.0",
    "express": "^4.1.1",
    "gulp": "^3.6.2",
    "gulp-compass": "^1.1.8",
    "gulp-concat": "^2.2.0",
    "gulp-connect": "^2.0.5",
    "gulp-connect-multi": "^1.0.8",
    "gulp-csso": "^0.2.8",
    "gulp-if": "^1.1.0",
    "gulp-imagemin": "^0.5.0",
    "gulp-inject": "^0.4.1",
    "gulp-jshint": "^1.5.5",
    "gulp-karma": "0.0.4",
    "gulp-livereload": "^1.3.1",
    "gulp-load-plugins": "^0.5.0",
    "gulp-ngmin": "^0.2.0",
    "gulp-open": "^0.2.8",
    "gulp-plumber": "^0.6.1",
    "gulp-rename": "^1.2.0",
    "gulp-rev": "^0.3.2",
    "gulp-rimraf": "^0.1.0",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^0.7.1",
    "gulp-size": "^0.3.0",
    "gulp-streamify": "0.0.5",
    "gulp-uglify": "^0.2.1",
    "gulp-util": "^2.2.14",
    "gulp-watch": "^0.6.4",
    "jshint-stylish": "^0.2.0",
    "karma": "^0.12.14",
    "karma-chrome-launcher": "^0.1.3",
    "karma-firefox-launcher": "^0.1.3",
    "karma-jasmine": "^0.1.5",
    "karma-safari-launcher": "^0.1.1",
    "lodash": "^2.4.1",
    "node-bourbon": "^1.2.2",
    "node-neat": "^1.3.0",
    "partialify": "^3.1.1",
    "path": "^0.4.9",
    "rimraf": "^2.2.6",
    "tiny-lr": "0.0.5",
    "vinyl-source-stream": "^0.1.1",
    "watchify": "^0.8.2"
  },
  "browserify": {
    "transform": [
      "deamdify",
      "partialify"
    ]
  },
  "watchify": {
    "transform": [
      "deamdify",
      "partialify"
    ]
  },
  "jshintConfig": {
    "asi": false
  }
}
